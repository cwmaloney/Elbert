    {
      "type": "node",
      "request": "launch",
      "name": "E131Test",
      "program": "${workspaceFolder}/E131Test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "holidayLights",
      "program": "${workspaceFolder}/holidayLights.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "FrameBuffer Test",
      "program": "${workspaceFolder}/testFrameBuffer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "EmulatorTransform Test",
      "program": "${workspaceFolder}/tests/EmulatorTransformTest.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Bmp Test",
      "program": "${workspaceFolder}/tests/BmpTest.js",
      "args": [
        "print3Lines"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Cheers",
      "program": "${workspaceFolder}/tests/RunCheers.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Images",
      "program": "${workspaceFolder}/tests/RunImages.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Messages",
      "program": "${workspaceFolder}/tests/RunMessages.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Emulator Server",
      "program": "${workspaceFolder}/displayEmulatorServer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
          "-u",
          "tdd",
          "--timeout",
          "999999",
          "--colors",
          "${workspaceFolder}/mochaTests"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "NameManager test",
      "program": "${workspaceFolder}/NameManager.js"
    },  ]
